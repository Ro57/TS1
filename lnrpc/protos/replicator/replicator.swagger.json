      "type": "object",
      "properties": {
        "id": {
          "type": "string",
          "title": "id — value is explicetely used by Replicator to uniquely identify related\nissuer later in order to ban him (discredite case), since\n\"identity_pubkey\", \"host\" may be changed"
        },
        "identity_pubkey": {
          "type": "string",
          "description": "identity_pubkey — the following field values are used to open payment\nchannel, invoices commands execution etc."
        },
        "host": {
          "type": "string",
          "title": "host is used to establish client connection to the issuer's node during\nsome RPC calls"
        }
      },
      "title": "IssuerInfo info about issuer server"
