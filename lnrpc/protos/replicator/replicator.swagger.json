          "items": {
            "$ref": "#/definitions/replicatorToken"
          }
        }
      }
    },
    "replicatorGetTokenBalancesResponse": {
      "type": "object",
      "properties": {
        "balances": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/replicatorTokenBalance"
