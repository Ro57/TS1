        "token": {
          "$ref": "#/definitions/tokendbToken",
          "title": "token — meta info about token"
        },
        "root": {
          "type": "string",
          "title": "root — hash of last block in blockchain"
        }
      },
      "title": "Token contain information about token to send it and store in DB"
    },
    "replicatorTokenBalance": {
      "type": "object",
      "properties": {
        "token": {
          "type": "string",
          "title": "token — name of token"
        },
        "available": {
          "type": "string",
          "format": "uint64",
          "title": "available godoc"
        },
        "frozen": {
          "type": "string",
          "format": "uint64",
          "title": "frozen godoc"
        }
      },
      "title": "TokenBalance godoc"
    },
    "replicatorTokenOffer": {
      "type": "object",
      "properties": {
        "token": {
          "type": "string",
          "title": "token — name of token"
        },
        "price": {
          "type": "string",
          "format": "uint64",
          "title": "price in PKT tokens"
        },
        "issuer_info": {
          "$ref": "#/definitions/replicatorIssuerInfo",
          "title": "issuer_info — contain info about token about the issuer server that \nissued the token"
        },
        "valid_until_seconds": {
          "type": "string",
          "format": "int64",
          "title": "valid_until_seconds — protects issuers from such a case, when potential\nbuyer successfully acquires dozens of signatures just in case if that \nissuer would raise up the price later. If a Token Wallet holder would\nlike to open channel with an outdated offer, Replicator would reject to\nregister this purchase and buyer stays unprotected"
        },
        "count": {
          "type": "string",
          "format": "uint64",
          "title": "count — the number of tokens to be issued"
        }
      },
      "title": "TokenOffer is used to send an offer to buy a token\nTODO: rework or remove"
