        },
        "creation_height": {
          "type": "string",
          "format": "uint64",
          "title": "creation_height — creation height in token blockchain"
        },
        "signature": {
          "type": "string",
          "title": "signature generated with old owner private key"
        }
      },
      "title": "Lock contain information about tokens and contract for transferring"
    },
    "protobufAny": {
      "type": "object",
      "properties": {
        "type_url": {
